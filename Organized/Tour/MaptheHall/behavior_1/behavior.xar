                                                    <content>
                                                        <![CDATA[import time
import json

class MyClass(GeneratedClass):
    last_left_found = None
    last_right_found = None
    last_found = None
    oppdirection = False

    #null_node = self.RoomNode("null", "null", )

    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def saveMap(self, filename, head):
        myMap = []

        while(head != None):
            myMap.append ({"name": head.getName(), "side": head.getSide()})
            head = head.getNextFound(False)

        file = open(filename, 'w')
        savedMap = json.dumps(myMap)
        file.write(savedMap)
        file.close()
        print("File saved!")

    def rewind(self, node):
        while(node.getLastFound(False) != None):
            node = node.getLastFound(False)

        return node

    def reset(self):
        self.last_left_found = None
        self.last_right_found = None
        self.last_found = None
        self.oppdirection = False

    def onInput_printList(self):
        listTraverser = self.last_found
        while(listTraverser.getLastFound(self.oppdirection) != None):
            listTraverser = listTraverser.getLastFound(self.oppdirection)

        print("Printing out the map:\n")

        while(listTraverser != None):
            time.sleep(0.5)
            print(listTraverser.toString())
            listTraverser = listTraverser.getNextFound(self.oppdirection)

    def onInput_printLastRoom(self):
        print("The last room I added was " + self.last_found.name)
        print("Map loaded!")


    def onInput_saveMap(self):
        firstNode = self.rewind(self.last_found)
        self.saveMap('map1.json', firstNode)

    def onInput_loadMap(self):
        print("Map loaded!")

        self.reset()

        map = json.loads(open('map1.json').read())

        for node in map:
            self.onInput_addToList([node['side'], node['name']])

        self.onInput_printList()

    def onInput_addToList(self, s):
        newRoom = self.RoomNode(s[1], s[0], self.last_left_found, self.last_right_found, self.last_found)

        if(s[0] == "left"):
            self.last_left_found = newRoom
        else:
            self.last_right_found = newRoom

        self.last_found = newRoom
        print("Added a new room to the list: " + self.last_found.toString())

    class RoomNode:
        name = ""
        side = ""
        last_left = None
        last_right = None
        last_found = None
        next_left = None
        next_right = None
        next_found = None

        def __init__(self, name, side, llf, lrf, lf):
            self.name = name
            self.side = side
            self.last_left = llf
            self.last_right = lrf
            self.last_found = lf

            if(side == "left"):
                if(self.last_left != None):
                    self.last_left.updateNextLeft(self)
                if(self.last_right != None):
                    self.last_right.updateNextLeft(self)
            else:
                if(self.last_right != None):
                    self.last_right.updateNextRight(self)
                if(self.last_left != None):
                    self.last_left.updateNextRight(self)

            if(self.last_found != None):
                self.last_found.updateNextFound(self)

        def getName(self):
            return self.name

        def getSide(self):
            return self.side

        def updateNextLeft(self, node):
            self.next_left = node

        def updateNextRight(self, node):
            self.next_right = node

        def updateNextFound(self, node):
            self.next_found = node

        def getNextFound(self, bool):
            if(bool == True):
                return self.last_found
            else:
                return self.next_found

        def getNextLeft(self, bool):
            if(bool == True):
                return self.last_right
            else:
                return self.next_left

        def getNextRight(self, bool):
            if(bool == True):
                return self.last_left
            else:
                return self.next_right

        def getLastFound(self, bool):
            if(bool == True):
                return self.next_found
            else:
                return self.last_found

        def getLastLeft(self, bool):
            if(bool == True):
                return self.next_right
            else:
                return self.last_left

        def getLastRight(self, bool):
            if(bool == True):
                return self.next_left
            else:
                return self.last_right

        def toString(self):
            if(self.last_left == None):
                ll = "None"
            else:
                ll = self.last_left.name
            if(self.last_right == None):
                lr = "None"
            else:
                lr = self.last_right.name
            if(self.next_found == None):
                n = "None"
            else:
                n = self.next_found.name
            if(self.last_found == None):
                l = "None"
            else:
                l = self.last_found.name
            if(self.next_left == None):
                nl = "None"
            else:
                nl = self.next_left.name
            if(self.last_right == None):
                nr = "None"
            else:
                nr = self.last_right.name


            return "{name: " + str(self.name) + ", side:" + self.side + "}"]]>
</content>
