                                                                            <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        pass

    def onUnload(self):
        pass

    def onInput_onStart(self):
        robotIP = "192.168.11.111"
        PORT = 9559

        try:
            memoryProxy = ALProxy("ALMemory", robotIP, PORT)
        except Exception, e:
            print "Could not create proxy to ALMemory"
            print "Error was: ", e

        sonarProxy = ALProxy("ALSonar", robotIP, PORT)
        sonarProxy.subscribe("myApplication")

        leftSonar = memoryProxy.getData("Device/SubDeviceList/US/Left/Sensor/Value")
        rightSonar = memoryProxy.getData("Device/SubDeviceList/US/Right/Sensor/Value")

        self.updateLeft(leftSonar)
        self.updateRight(rightSonar)

        sonarProxy.unsubscribe("myApplication")

        self.onStopped()
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
