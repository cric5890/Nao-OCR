                                                                    <script language="4">
                                                                        <content>
                                                                            <![CDATA[import time
import math
import threading

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        self.motion = ALProxy("ALMotion")
        self.shouldRun = False
        self.x = 0
        self.y = 0
        self.theta = 0
        self.timer = None
        self.lock = threading.RLock()

        successLeft = self.motion.setCollisionProtectionEnabled("LArm", False);
        successRight = self.motion.setCollisionProtectionEnabled("RArm", False);
        if (not successLeft or not successRight):
            print "Failed to turn off collision detection"

    def onUnload(self):
        self.lock.acquire()
        try:
            self.shouldRun = False
            if self.timer:
                self.timer.cancel()
                self.timer = None

            self.x = 0
            self.y = 0
            self.theta = 0
            self.motion.moveToward(0, 0, 0)
            self.motion.waitUntilMoveIsFinished()
        finally:
            self.lock.release()

    def onInput_onStop(self):
        self.lock.acquire()
        try:
            self.onUnload()
            self.onStopped()
        finally:
            self.lock.release()

    def onInput_onStart(self):
        self.lock.acquire()
        try:
            self.shouldRun = True
            print("theta from onStart: " + str(self.theta))
            self.updateMovement([0,0,0])
        finally:
            self.lock.release()

    def onInput_updateMovement(self, s):
        self.updateMovement(s)

    def updateMovement(self, s):
        self.lock.acquire()
        self.x = s[0]
        self.y = s[1]
        self.theta = s[2]
        try:
            if self.timer:
                self.timer.cancel()
                self.timer = None
            enableArms = self.getParameter("Arms movement enabled")
            self.motion.setMoveArmsEnabled(enableArms, enableArms)

            self.motion.moveToward(self.x, self.y, self.theta)
        finally:
            self.lock.release()]]>
</content>
