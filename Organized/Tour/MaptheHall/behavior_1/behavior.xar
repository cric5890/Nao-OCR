                                                                    <script language="4">
                                                                        <content>
                                                                            <![CDATA[class MyClass(GeneratedClass):
    leftSonar = 0
    rightSonar = 0
    theta = 0.0

    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        if (self.leftSonar < 0.35 and self.leftSonar != 0.0) and (self.rightSonar < 0.35 and self.rightSonar != 0.0):
            self.imStuck()
            pass
        elif (self.leftSonar < 1 and self.leftSonar != 0.0) or (self.rightSonar < 1 and self.rightSonar != 0.0):
            if self.leftSonar < self.rightSonar:
                self.theta = self.leftSonar - 1
            else:
                self.theta = 1 - self.rightSonar
        else:
            self.theta = 0

        print("left sonar:" + str(self.leftSonar) + "\t right sonar: "+ str(self.rightSonar)) + "\t theta: " + str(self.theta)


        if self.theta >= 0.8 or self.theta <= -0.8:
            if self.theta >= 0.8:
                print ("GOING TO THE LEFT " + str(self.theta))
            else:
                print ("GOING TO THE RIGHT " + str(self.theta))

        self.onStopped([1, 0, self.theta])
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped([0, 0, 0]) #activate the output of the box

    def onInput_updateLeftSonar(self, p):
        self.leftSonar = p
        pass

    def onInput_updateRightSonar(self, p):
        self.rightSonar = p
        pass]]>
